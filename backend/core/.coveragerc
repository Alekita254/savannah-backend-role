[run]
branch = True
source =
    ../myuser
    ../products
    .
omit =
    */migrations/*
    */tests/*
    */__init__.py
    manage.py
    */settings.py
    venv/*

[report]
exclude_lines =
    pragma: no cover
    def __str__
    raise NotImplementedError
    if __name__ == .__main__.:
    if settings.DEBUG